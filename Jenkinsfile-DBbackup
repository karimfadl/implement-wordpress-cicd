pipeline {
    agent any
    stages {
        stage('DB-BackupDeploy') {
            when {
                branch 'master'
            }
            steps {
                kubernetesDeploy(
                    kubeconfigId: 'kubeconfig',
                    configs: 'backup-mysql.yml',
                    enableConfigSubstitution: true
                )
            }
        }

    }
}
